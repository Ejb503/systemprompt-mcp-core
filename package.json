{
  "name": "systemprompt-agent-server",
  "version": "1.0.1",
  "description": "A Model Context Protocol server",
  "type": "module",
  "bin": {
    "systemprompt-agent-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"import('fs').then(fs => fs.chmodSync('build/index.js', '755'))\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" API_KEY=test-api-key BASE_URL=https://api.systemprompt.io/v1 jest",
    "test:watch": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-module-name-mapper": "^0.1.5",
    "ts-jest": "^29.1.2",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ejb503/systemprompt-mcp-core"
  },
  "keywords": [
    "systemprompt",
    "mcp",
    "model-context-protocol",
    "llm",
    "ai",
    "agent"
  ],
  "author": "SystemPrompt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ejb503/systemprompt-mcp-core/issues"
  },
  "homepage": "https://systemprompt.io",
  "engines": {
    "node": ">=18.0.0"
  }
}
